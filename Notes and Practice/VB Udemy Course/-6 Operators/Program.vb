Imports System

Module Program
    Sub Main()
        'ARITHMETIC OPERATORS: +, -, *, %, Mod are arithmetic operators
        Console.WriteLine(5 + 9)

        'COMPARISON OPERATORS: =, <>, >, <, >=, <= are comparison operators
        Console.WriteLine(5 = 9) 'returns false because = is a comparison operator.
        Console.WriteLine(5 = 5) 'returns true because = is a comparison operator.
        Console.WriteLine(5 < 9) 'returns true, because 5 IS greater than 9

        'LOGICAL OPERATORS: AND, OR, NOT, XOR

    End Sub
End Module
